#include<stdio.h>

int main()
{
    int numbers[1010] = {0};
    int N;
    int A1,A2,A3,A4,A5;
    A1 = A2 = A3 = A4 = A5 = 0;
    int numA4 = 0;
    while(scanf("%d",N) != EOF){
        for(int i = 0;i < N;i++){
            scanf("%d",&numbers[i]);
            if(numbers[i] % 5 == 0){
                if(numbers[i] % 2 ==0){
                    A1 += numbers[i];
                }
            }
            else if(numbers[i] % 5 == 1){
                if(i % 2 != 0){
                    A2 -= numbers[i];
                }
                else{
                    A2 +=numbers[i];
                }
            }
            else if(numbers[i] % 5 == 2){
                A3++;
            }
            else if(numbers[i] % 5 == 3){
                A4 += numbers[i];
                numA4++;
            }
            else if(numbers[i] % 5 == 4){
                if(numbers[i] > A5){
                    A5 = numbers[i];
                }
            }
        }
        printf("%d %d %d %f %d",A1,A2,A3,A4 / numA4,A5);

    }
    return 0;
}
